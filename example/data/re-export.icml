<Document DOMVersion="8.0" Self="ntz2icml_doc">
  <RootCharacterStyleGroup Self="ntz2icml_character_styles">
    <CharacterStyle Self="$ID/[No character style]" Name="$ID/[No character style]"/>
    <CharacterStyle Self="CharacterStyle/GT Zeitung Auszeichnung ExtraBold" Name="GT Zeitung Auszeichnung ExtraBold"/>
    <CharacterStyle Self="CharacterStyle/GT Zeitung Auszeichnung Kursiv" Name="GT Zeitung Auszeichnung Kursiv"/>
    <CharacterStyle Self="CharacterStyle/GT Zeitung" Name="GT Zeitung"/>
  </RootCharacterStyleGroup>
  <RootParagraphStyleGroup Self="ntz2icml_paragraph_styles">
    <ParagraphStyle Self="$ID/NormalParagraphStyle" Name="$ID/NormalParagraphStyle"/>
